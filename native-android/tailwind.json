{
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"mt-3": {
		"style": {
			"marginTop": 12
		}
	},
	"mt-6": {
		"style": {
			"marginTop": 24
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-10": {
		"style": {
			"height": 40
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-col": {
		"style": {
			"flexDirection": "column"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-2": {
		"style": {
			"borderTopWidth": 2,
			"borderRightWidth": 2,
			"borderBottomWidth": 2,
			"borderLeftWidth": 2
		}
	},
	"border-gray-300": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(209 213 219 / var(--tw-border-opacity))"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"p-6": {
		"style": {
			"paddingTop": 24,
			"paddingRight": 24,
			"paddingBottom": 24,
			"paddingLeft": 24
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-xl": {
		"style": {
			"fontSize": 20,
			"lineHeight": 28
		}
	},
	"font-semibold": {
		"style": {
			"fontWeight": "600"
		}
	}
}